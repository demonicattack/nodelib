{
  "name": "@nodelib/http.requests",
  "version": "0.0.0",
  "description": "A library for efficiently working with a HTTP requests",
  "license": "MIT",
  "repository": "https://github.com/nodelib/nodelib/tree/master/packages/http/requests",
  "keywords": [
    "NodeLib",
    "http",
    "https",
    "got",
    "request",
    "requests"
  ],
  "engines": {
    "node": ">= 8"
  },
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "clean": "rimraf {tsconfig.tsbuildinfo,out}",
    "lint": "eslint \"src/**/*.ts\" --cache",
    "compile": "tsc -b .",
    "compile:watch": "tsc -p . --watch --sourceMap",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run compile && npm run lint && npm test",
    "watch": "npm run clean && npm run compile:watch"
  },
  "dependencies": {
    "got": "11.7.0"
  }
}
