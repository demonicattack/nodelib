{
  "name": "@nodelib/benchmark.meter",
  "version": "1.0.1",
  "description": "The utilities for work with metrics for @nodelib/benchmark",
  "license": "MIT",
  "repository": "https://github.com/nodelib/nodelib/tree/master/packages/benchmark/benchmark.meter",
  "keywords": [
    "NodeLib",
    "benchmark",
    "bench",
    "meter"
  ],
  "engines": {
    "node": ">= 6"
  },
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "clean": "rimraf out",
    "lint": "tslint \"src/**/*.ts\" -p . -t stylish",
    "compile": "tsc -b .",
    "compile:watch": "tsc -p . --watch --sourceMap",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run lint && npm run compile && npm test",
    "watch": "npm run clean && npm run compile:watch"
  }
}
